{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://raw.githubusercontent.com/yoku-app/Schema/main/schemas/user/dto/user-partial-dto.schema.json",
    "title": "UserPartialDTO",
    "type": "object",
    "required": ["id", "name", "email"],
    "properties": {
        "id": { "type": "string", "format": "uuid" },
        "name": { "type": "string" },
        "dob": {
            "type": "string",
            "format": "date-time",
            "tsType": "Date"
        },
        "email": { "type": "string", "format": "email" },
        "avatarUrl": { "type": "string", "format": "uri" }
    },
    "additionalProperties": false
}
