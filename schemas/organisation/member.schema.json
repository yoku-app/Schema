{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://raw.githubusercontent.com/yoku-app/Schema/refs/heads/main/schemas/organisation/member.schema.json",
    "title": "OrganisationMember",
    "type": "object",
    "required": ["id", "memberSince", "user", "organisation"],
    "properties": {
        "id": {
            "type": "object",
            "required": ["organisationId", "userId"],
            "properties": {
                "organisationId": { "type": "string", "format": "uuid" },
                "userId": { "type": "string", "format": "uuid" }
            },
            "additionalProperties": false
        },
        "memberSince": {
            "type": "string",
            "format": "date-time",
            "tsType": "Date"
        },
        "user": {
            "$ref": "https://raw.githubusercontent.com/yoku-app/Schema/refs/heads/main/schemas/user/profile.schema.json"
        },
        "organisation": {
            "$ref": "https://raw.githubusercontent.com/yoku-app/Schema/refs/heads/main/schemas/organisation/organisation.schema.json"
        }

    },
    "additionalProperties": false
}
