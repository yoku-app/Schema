{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "OrgMemberDTO",
    "type": "object",
    "required": [
        "id",
        "displayName",
        "email",
        "avatarUrl"
    ],
    "properties": {
        "id": { "type": "string", "format": "uuid" },
        "displayName": { "type": "string" },
        "email": { "type": "string", "format": "email" },
        "avatarUrl": {
            "oneOf": [{ "type": "string", "format": "uri" }, { "type": "null" }]
        }
    },
    "additionalProperties": false
}