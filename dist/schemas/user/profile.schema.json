{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "UserProfile",
    "type": "object",
    "required": [
        "userId",
        "email",
        "displayName",
        "createdAt",
        "focus",
        "avatarUrl"
    ],
    "properties": {
        "userId": {
            "type": "string",
            "format": "uuid"
        },
        "phone": {
            "type": "string",
            "nullable": true
        },
        "focus": {
            "oneOf": [
                {
                    "type": "string",
                    "enum": [
                        "RESPONDENT",
                        "CREATOR",
                        "HYBRID"
                    ]
                },
                {
                    "type": "null"
                }
            ]
        },
        "dob": {
            "type": "string",
            "format": "date",
            "nullable": true
        },
        "email": {
            "type": "string",
            "format": "email"
        },
        "displayName": {
            "type": "string"
        },
        "avatarUrl": {
            "oneOf": [
                {
                    "type": "string",
                    "format": "uri"
                },
                {
                    "type": "null"
                }
            ]
        },
        "createdAt": {
            "type": "string",
            "format": "date-time"
        },
        "updatedAt": {
            "type": "string",
            "format": "date-time",
            "nullable": true
        },
        "onboardingCompletion": {
            "type": "object",
            "nullable": true,
            "properties": {
                "respondent": {
                    "type": "string",
                    "format": "date-time",
                    "nullable": true
                },
                "creator": {
                    "type": "string",
                    "format": "date-time",
                    "nullable": true
                },
                "core": {
                    "type": "string",
                    "format": "date-time",
                    "nullable": true
                }
            }
        }
    },
    "additionalProperties": false
}